#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    int n,k;
	    cin>>n>>k;
	    int a[n];
	    
	    int x;
	    int y=0;
	    for(int i=0; i<n; i++){
	        cin>>a[0];
	        x=a[0]+k;
	        if(x%7==0){
	            y++;
	        }
	       
	    }
	    cout<<y<<endl;
	}
return 0;
}
