#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    int a,b,c,x;
	    cin>>a>>b>>c;
	    x=a;
	    if(x<=b){
	        x=b;
	        if(x>=c){
	        
	        cout<<"YES"<<endl;
	    }
	    else{
	        cout<<"NO"<<endl;
	    }
	    }
	    else{
	        cout<<"NO"<<endl;
	    }
	}
	

}
