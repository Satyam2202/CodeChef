#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    int n;
	    cin>>n;
	    string s[n];
	    int x=0, y=0;
	    for(int i=0; i<n; i++){
	        cin>>s[i];
	        if(s[i]=="START38"){
	            x++;
	        }
	        else{
	            y++;
	        }
	    }
	    cout<<x<<" "<<y<<endl;
	}

}
