#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    int a,b,x,y;
	    cin>>a>>b>>x>>y;
	    if (a * y == b * x) {
            cout << "BOTH" << endl;
        }
        else if (a * y < b * x) {
            cout << "CHEF" << endl;
        }
        else {
            cout << "CHEFINA" << endl;
        }
	}

}
