#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    int n;
	    cin>>n;
	    int a[n];
	    
	    int q=0;
	    for(int i=0; i<n; i++){
	        cin>>a[i];
	        
	        }
	        for(int i=0; i<n; i++){
	            if(a[i]>=10&&a[i]<=60){
	            q=q+1;
	            
	        }
	        
	        }
	    cout<<q<<endl;
	}

}
