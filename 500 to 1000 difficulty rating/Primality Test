#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    int n,i;
	    cin>>n;
	    if(n<=1){
	        cout<<"NO"<<endl;
	    }
	    else{
	        for( i=2; i<n; i++){
	            if(n%i==0){
	               break;
	            }
	            
	        }
	       if(i==n){
	            cout<<"YES"<<endl;
	       }
	       else{
	            cout<<"NO"<<endl;
	 }
	        
	    }
	    
	}

}
