#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    int x;
	    cin>>x;
	    if(x>=7){
	        cout<<"HEAVY"<<endl;
	    }
	    else if(x>=3){
	        cout<<"MODERATE"<<endl;
	    }
	    else{
	        cout<<"LIGHT"<<endl;
	    }
	}

}
