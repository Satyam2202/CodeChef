#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int x1,y1,a,b,x2,y2;
	cin>>x1>>y1;
	x1=x1*2;
	y1=y1*1;
	a=x1+y1;
	cin>>x2>>y2;
	x2=x2*2;
	y2=y2*1;
	b=x2+y2;
	if(a==b){
	    cout<<"Equal"<<endl;
	}
	else if(a>b){
	    cout<<"Messi"<<endl;
	}
	else{
	    cout<<"Ronaldo"<<endl;
	}
	

}
