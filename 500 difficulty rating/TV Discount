#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    int a,b,c,d;
	    cin>>a>>b>>c>>d;
	    a=a-c;
	    b=b-d;
	    if(a>b){
	        cout<<"Second"<<endl;
	        
	    }
	    else if (a==b){
	        cout<<"Any"<<endl;
	    }
	    else{
	        cout<<"First"<<endl;
	    }
	}

}
